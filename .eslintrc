{
    "extends": [
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "no-console":0,
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "plugins": [
      "prettier"
    ],

    "env":{
      "es6": true
    },

    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    }
  }